<h1> uhhh particles? </h1>
<ul>
    {{#particles}}
        {{#claimed}}
            <li
                particle
                data-fb-key="{{key}}"
                data-access-key="{{access_key}}"
                data-claimed="{{claimed}}"
                data-nickname="{{nickname}}"
            >
                nickname: {{nickname}}
            </li>
        {{/claimed}}
        {{^claimed}}
            <li
                particle
                data-fb-key="{{key}}"
                data-claimed="{{claimed}}"
            >
                unclaimed
            </li>
        {{/claimed}}
    {{/particles}}
</ul>

<script src="../static/javascript/ParticleHelper.js"></script>
